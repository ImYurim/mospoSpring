<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.developia.mospo.models.MovieDAO">
	<select id="getMovie" parameterType="String" resultType="MovieDTO">
		select * 
		from movie
		where title = #{title}	
	</select>
	
</mapper>